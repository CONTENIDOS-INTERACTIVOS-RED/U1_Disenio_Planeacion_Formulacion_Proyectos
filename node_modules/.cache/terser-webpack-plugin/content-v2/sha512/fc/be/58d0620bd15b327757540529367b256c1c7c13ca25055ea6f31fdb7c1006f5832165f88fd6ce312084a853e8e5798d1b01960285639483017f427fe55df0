cesario y asegurar que el proyecto esté alineado con los objetivos establecidos.\")])])])},function(){var a=this,e=a.$createElement,s=a._self._c||e;return s(\"div\",{staticClass:\"titulo-segundo\",attrs:{id:\"t_2_4\",\"data-aos\":\"flip-up\"}},[s(\"h2\",[s(\"span\",[a._v(\"2.4\")]),a._v(\" Cierre del proyecto\")])])},function(){var a=this,e=a.$createElement,t=a._self._c||e;return t(\"div\",{staticClass:\"col-lg-4 order-lg-2\"},[t(\"figure\",[t(\"img\",{staticClass:\"mb-4 mb-lg-0\",attrs:{src:s(\"144d\"),\"data-aos\":\"zoom-in\"}})])])},function(){var a=this,e=a.$createElement,s=a._self._c||e;return s(\"div\",{staticClass:\"anexo__texto\"},[s(\"p\",[s(\"strong\",[a._v(\"Anexo. \")]),a._v(\" Cierre del proyecto\")])])}],o=s(\"3cd8\"),c={name:\"Tema2\",components:{BannerInterno:o[\"default\"]},mounted:function(){var a=this;this.$nextTick((function(){a.$aosRefresh()}))}},r=c,n=s(\"2877\"),l=Object(n[\"a\"])(r,t,i,!1,null,null,null);e[\"default\"]=l.exports},\"791e\":function(a,e,s){a.exports=s.p+\"img/5.ced6b095.png\"},\"7a59\":function(a,e,s){a.exports=s.p+\"img/7.4cbe0155.png\"},\"7ebd\":function(a,e,s){a.exports=s.p+\"img/6.b801536b.png\"},8385:function(a,e,s){\"use strict\";s(\"d36b\")},\"89f4\":function(a,e,s){a.exports=s.p+\"img/11.b7afac03.png\"},\"95e6\":function(a,e,s){a.exports=s.p+\"img/3.1fb45f16.png\"},a91d:function(a,e,s){a.exports=s.p+\"img/8.f172b291.svg\"},b7da:function(a,e,s){a.exports=s.p+\"img/2.68c8a2d0.png\"},bd18:function(a,e,s){a.exports=s.p+\"img/tab-2-1.955746de.svg\"},d326:function(a,e,s){a.exports=s.p+\"img/slider-2.761c5a99.png\"},d36b:function(a,e,s){},d884:function(a,e,s){a.exports=s.p+\"img/slider-3.6a609304.png\"},ddff:function(a,e,s){a.exports=s.p+\"img/slider-4.56d2b602.png\"},e73b:function(a,e,s){a.exports=s.p+\"img/slider-1.2bae4011.png\"},eabe:function(a,e,s){a.exports=s.p+\"img/4.a867e966.svg\"},f103:function(a,e,s){a.exports=s.p+\"img/figura2.ed42c513.png\"},f36c:function(a,e,s){a.exports=s.p+\"img/12.98305616.png\"},f563:function